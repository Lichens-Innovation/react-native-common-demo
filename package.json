{
  "name": "react-native-common-demo",
  "displayName": "RNCommonApp",
  "description": "React Native Common Kitchen Sink App",
  "releaseNotes": "",
  "author": "Lichens Innovation",
  "repository": "https://github.com/Lichens-Innovation/react-native-common-demo.git",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --clear",
    "start:tunnel": "expo start --clear --tunnel",
    "local:dev:activate": "node ./scripts/local-dev-activate.js",
    "android": "expo run:android",
    "android:device": "expo run:android --device",
    "android:device:release": "expo run:android --device --variant release",
    "ios": "expo run:ios",
    "ios:device": "expo run:ios --device",
    "ios:device:release": "expo run:ios --device --configuration Release",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "sitemap": "npx expo-router-sitemap",
    "clean:prebuild": "expo prebuild --clean",
    "clean:prebuild:ios": "expo prebuild --platform ios --clean",
    "clean:prebuild:android": "expo prebuild --platform android --clean",
    "clean:pods": "npx rimraf ios/Pods ios/Podfile.lock && cd ios && pod install && cd ..",
    "clean:node": "npx rimraf node_modules yarn.lock",
    "ip:macos": "ipconfig getifaddr en0",
    "ip:windows": "ipconfig",
    "ip:linux": "hostname -I",
    "expo:doctor": "npx expo-doctor --verbose",
    "expo:upgrade:check": "npx expo install --check",
    "expo:upgrade:latest": "npx expo install expo@latest --fix",
    "expo:install:fix": "expo install --fix",
    "expo:build:android": "npx expo run:android",
    "expo:build:ios": "npx expo run:ios",
    "eas:whoami": "eas whoami",
    "eas:env:var:update": "eas env:update",
    "eas:env:var:delete": "eas env:delete",
    "eas:build:ios:development": "yarn generate:version && eas build --platform ios --profile development",
    "eas:build:ios:development:simulator": "yarn generate:version && eas build --platform ios --profile development-simulator",
    "eas:build:ios:preview": "yarn generate:version && eas build --platform ios --profile preview",
    "eas:build:ios:preview:local": "yarn generate:version && eas build --platform ios --profile preview --local",
    "eas:build:android:development": "yarn generate:version && eas build --platform android --profile development",
    "eas:build:android:preview": "yarn generate:version && eas build --platform android --profile preview",
    "eas:build:android:preview:local": "yarn generate:version && eas build --platform android --profile preview --local",
    "eas:run:ios": "eas build:run -p ios --latest",
    "eas:run:android": "eas build:run -p android --latest",
    "eas:update:ios:preview": "node scripts/ota-update.js ios",
    "eas:update:android:preview": "node scripts/ota-update.js android",
    "generate:version": "node scripts/generate-version",
    "typecheck": "npx tsc --noEmit",
    "postinstall": "patch-package && license-checker-rseidelsohn --json --relativeLicensePath --relativeModulePath --out assets/licenses.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@Lichens-Innovation/react-native-common": "^2.9.1",
    "@expo/vector-icons": "^15.0.3",
    "@gorhom/bottom-sheet": "^5.2.4",
